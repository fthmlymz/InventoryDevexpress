{
  "GlobalConfiguration": {
    "BaseUrl": "${GatewaySettings__BaseUrl}",
    "Hosts": {
      "KeycloakServiceInventory": "${GatewaySettings__Hosts__KeycloakServiceInventory}",
      "KeycloakServiceInventoryRealmId": "${GatewaySettings__Hosts__KeycloakServiceInventoryRealmId}",
      "InventoryManagementAPI": "${GatewaySettings__Hosts__InventoryManagementAPI}",
      "FileTransferManagerAPI": "${GatewaySettings__Hosts__FileTransferManagerAPI}"
    }
  },

  "Routes": [
    //Keycloak - Get Access Token
    {
      "UpstreamPathTemplate": "/gateway/inventory/user/token/{everything}",
      "UpstreamHttpMethod": [ "GET", "POST" ],
      "DangerousAcceptAnyServerCertificateValidator": true,
      "DownstreamPathTemplate": "/realms/{KeycloakServiceInventoryRealmId}/protocol/openid-connect/token",
      "DownstreamHostAndPorts": [
        {
          "Host": "{KeycloakServiceInventory}"
        }
      ]
    },
    //Keycloak - Logout user
    {
      "UpstreamPathTemplate": "/gateway/inventory/user/logout",
      "UpstreamHttpMethod": [ "POST" ],
      "DangerousAcceptAnyServerCertificateValidator": true,
      "DownstreamPathTemplate": "/realms/{KeycloakServiceInventoryRealmId}/protocol/openid-connect/logout",
      "DownstreamHostAndPorts": [
        {
          "Host": "{KeycloakServiceInventory}"
        }
      ]
    },
    //Keycloak - Get user info
    {
      "UpstreamPathTemplate": "/gateway/inventory/user/info",
      "UpstreamHttpMethod": [ "GET" ],
      "DangerousAcceptAnyServerCertificateValidator": true,
      "DownstreamPathTemplate": "/realms/{KeycloakServiceInventoryRealmId}/protocol/openid-connect/userinfo",
      "DownstreamHostAndPorts": [
        {
          "Host": "{KeycloakServiceInventory}"
        }
      ]
    },
    //Keycloak - search users
    {
      "UpstreamPathTemplate": "/gateway/inventory/user/{everything}",
      "UpstreamHttpMethod": [ "GET" ],
      "DangerousAcceptAnyServerCertificateValidator": true,
      "DownstreamPathTemplate": "/admin/realms/{KeycloakServiceInventoryRealmId}/users?{everything}",
      "DownstreamHostAndPorts": [
        {
          "Host": "{KeycloakServiceInventory}"
        }
      ]
    },




